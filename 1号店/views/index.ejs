<link rel="stylesheet" href="/stylesheets/bootstrap.css">
<style>
	body{
		background: #f5f5f5;
	}
	/* Make the image fully responsive */
	.carousel-inner img {
		width: 100%;
		height: 100%;
	}
	.sing_text_font {
		font-size: 14px;
		color: rgba(255,255,255,0.8);
		display: block;
		margin-left: 55px;
		height: 22px;
		float: left;
		font-weight: bold;
		cursor: pointer;
	}
	.single_top {
		width: 210px;
		height: 135px;
		background-position: center;
		background-repeat: no-repeat;
		text-align: center;
		position: relative;
	}
	.single_bottom {
		height: 81px;
		width: 150px;
		margin: 0 auto;
		position: relative;
	}
	.s_title {
		width: 153px;
		height: 20px;
		overflow: hidden;
		white-space: nowrap;
		text-overflow: ellipsis;
		margin-top: 10px;
	}
	.s_con {
		height: 33px;
		padding-top: 12px;
		position: relative;
		overflow: hidden;
		text-align: center;
	}
	.s_num {
		display: inline-block;
		font-family: Tahoma,"Microsoft YaHei";
		color: #e50113;
		font-weight: bold;
		font-size: 14px;
	}
	.s_num_underline {
		text-decoration: line-through;
		font-size: 12px;
		color: #999;
		font-weight: normal;
		margin-left: 4px;
	}
	.qua_tit_spanFirst {
		height: 35px;
		margin-left: 20px;
		font-size: 26px;
		display: inline-block;
		cursor: pointer;
		max-width: 190px;
		overflow: hidden;
    }
	.qua_tit_spanLast {
		margin-left: 8px;
		font-size: 16px;
		margin-top: 27px;
		height: 21px;
		cursor: pointer;
		display: inline-block;
		max-width: 208px;
		overflow: hidden;
		position: relative;
		top: -3px;
	}
	.qua_show_con {
