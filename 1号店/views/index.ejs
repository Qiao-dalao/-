<script>
var dong = document.querySelectorAll('.dong')
for(var i=0;i<dong.length;i++){
	dong[i].onmouseover=function(){
		this.style.boxShadow='0 0 10px #b77347'
		this.style.border='1px solid #b77347'
		this.style.transition='500ms'
	}
	dong[i].onmouseleave=function(){
		this.style.boxShadow='none'
		this.style.border='none'
		this.style.transition='500ms'
	}
}
</script>
